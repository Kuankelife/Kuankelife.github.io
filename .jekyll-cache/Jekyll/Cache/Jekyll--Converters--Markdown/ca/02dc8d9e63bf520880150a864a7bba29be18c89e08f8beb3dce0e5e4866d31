$$
xxxxxxxxxx \mathbf<span class="p">{</span>V<span class="p">}</span>_1 \times \mathbf<span class="p">{</span>V<span class="p">}</span>_2 <span class="p">=</span>  \begin<span class="p">{</span>vmatrix<span class="p">}</span> \mathbf<span class="p">{</span><span class="k">i</span><span class="p">}</span> &amp; \mathbf<span class="p">{</span><span class="k">j</span><span class="p">}</span> &amp; \mathbf<span class="p">{</span><span class="k">k</span><span class="p">}</span> \\\frac<span class="p">{</span>\partial X<span class="p">}{</span>\partial <span class="k">u</span><span class="p">}</span> &amp;  \frac<span class="p">{</span>\partial Y<span class="p">}{</span>\partial <span class="k">u</span><span class="p">}</span> &amp; <span class="m">0</span> \\\frac<span class="p">{</span>\partial X<span class="p">}{</span>\partial <span class="k">v</span><span class="p">}</span> &amp;  \frac<span class="p">{</span>\partial Y<span class="p">}{</span>\partial <span class="k">v</span><span class="p">}</span> &amp; <span class="m">0</span> \\\end<span class="p">{</span>vmatrix<span class="p">}</span>
